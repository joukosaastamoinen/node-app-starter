{
  "name": "node-app-starter",
  "version": "1.0.0",
  "description": "Node app starter",
  "main": "src/index.js",
  "scripts": {
    "start": "node build/index.js",
    "lint": "standard --ignore-path .gitignore '**/*.js'",
    "build": "babel src --out-dir build --ignore test.js",
    "test": "tape -r babel-register 'src/**/*.test.js' | tap-dot",
    "test:watch": "nodemon --ignore build/ -q -x 'npm test'",
    "dev": "npm run test:watch & babel-watch src/index.js & wait"
  },
  "author": "Jouko Saastamoinen <joukosaastamoinen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "babel-watch": "2.0.7",
    "nodemon": "1.17.1",
    "standard": "10.0.3",
    "tap-dot": "1.0.5",
    "tape": "4.8.0"
  },
  "engines": {
    "node": "9.0.0"
  }
}
